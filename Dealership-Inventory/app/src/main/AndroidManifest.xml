<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dealershipinventory">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dealer_logo_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/dealer_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Cars For Sale"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CarDetails" />
        <activity android:name=".edit_car" />
        <activity android:name=".add_car" />
        <activity android:name=".Login"
            android:theme="@style/Theme.AppCompat"></activity>
    </application>

</manifest>